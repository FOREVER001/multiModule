<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#FF4081</color>
    <color name="colorPrimary">#2196F3</color>
    <color name="colorPrimaryDark">#1976D2</color>
    <color name="color_333">#333333</color>
    <color name="color_ffffff">#ffffff</color>
    <color name="module1_757575">#757575</color>
    <declare-styleable name="CircleImageView"><attr format="dimension" name="border_width"/><attr format="color" name="border_color"/></declare-styleable>
    <declare-styleable name="StatusBarHeightView"><attr format="integer" name="use_type">
            <enum name="use_height" value="0"/>
            <enum name="use_padding_top" value="1"/>
        </attr></declare-styleable>
    <dimen name="dp15">15dp</dimen>
    <dimen name="dp_12">12dp</dimen>
    <dimen name="dp_16">16dp</dimen>
    <dimen name="dp_28">28dp</dimen>
    <dimen name="dp_8">8dp</dimen>
    <string name="app_name">BaseLibs</string>
    <style name="DrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="spinBars">true</item>
        <item name="color">@android:color/white</item>
    </style>
    <style name="OverFlowMenuTheme" parent="Theme.AppCompat.NoActionBar">
        <!--<item name="actionMenuTextColor">@color/color_212121</item>-->
        <!-- 设置Menu菜单的背景色 -->
        <item name="android:itemBackground">@color/color_ffffff</item>
        <!-- 设置Menu菜单的字体颜色 -->
        <item name="android:textColorPrimary">@android:color/black</item>

        <item name="overlapAnchor">false</item>
    </style>
    <style name="app_splashTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@mipmap/splash</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="toolbar_theme" parent="@style/ThemeOverlay.AppCompat.Light">
        <!--NavigationIcon的颜色-->
        <item name="colorControlNormal">@color/color_ffffff</item>
        <item name="android:textColorPrimary">@color/color_ffffff</item>
    </style>
</resources>